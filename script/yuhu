function arthub(msg, delay, color, title, desc)
    return ArtHub:MakeNotify({
        Title = title or "ArtHub",
        Description = desc or "Notification",
        Content = msg or "Content",
        Color = color or Color3.fromRGB(255, 0, 255),
        Delay = delay or 4
    })
end

