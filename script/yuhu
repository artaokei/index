                while _G.HideRod do
                    for _, a in ipairs(workspace.Characters:GetChildren()) do
                        local b = a:FindFirstChild("!!!EQUIPPED_TOOL!!!");
                        if b then
                            b:Destroy();
                        end;
                    end;
                    task.wait(1);
                end;
            end);
        end;
    end
});

