-- WEBHOOK SECTION
local WebhookSection = tab.Webhook:AddSection("WEBHOOK")

-- [[ WEBHOOK SETTINGS & FUNCTIONS ]]
local rareti = { "Mythic", "SECRET" }
local webactive = false
local hookConnection = nil
local WEBHOOK_URL = ""

local function getTierColor(tierName)
    local colors = {
        ["Common"] = 0x7289DA,
        ["Uncommon"] = 0x57F287,
        ["Rare"] = 0x3498DB,
        ["Epic"] = 0x9B59B6,
        ["Legendary"] = 0xF1C40F,
        ["Mythic"] = 0xE91E63,
        ["SECRET"] = 0xFF00FF
    }
    return colors[tierName] or 0x7289DA
end

local function getTierName(tierNumber)
    local tierMap = {
        [1] = "Common",
        [2] = "Uncommon",
        [3] = "Rare",
        [4] = "Epic",
        [5] = "Legendary",
        [6] = "Mythic",
        [7] = "SECRET"
    }
    return tierMap[tierNumber] or "Unknown"
end

-- Fungsi mengambil gambar thumbnail item dari Roblox API
local function getImg(idIcon)
    local getThumb = tostring(idIcon):match("rbxassetid://(%d+)") or tostring(idIcon):match("(%d+)")
    if not getThumb then return nil end

    local url = "https://thumbnails.roblox.com/v1/assets?assetIds=" .. getThumb .. "&type=Asset&size=420x420&format=Png"
    local success, result = pcall(function()
        return SERVICES.HttpService:JSONDecode(game:HttpGet(url))
    end)

    if success and result and result.data and result.data[1] then
        return result.data[1].imageUrl
    end
    return nil
end

local function sendFishWebhook(fishName, tierName, fishWeight, fishMutation, fishPrice, icn)
    if not webactive or WEBHOOK_URL == "" or not WEBHOOK_URL:find("discord.com") then return end

    local imageUrl = getImg(icn)
    local payload = {
        username = "Fishing Logger",
        embeds = { {
            title = " " .. fishName,
            color = getTierColor(tierName),
            description = string.format(
                "**Weight**: `%s kg`\n**Rarity**: `%s`\n**Sell Price**: `$%s`\n**Mutation**: `%s`\n**Catch Date**: `%s`",
                fishWeight or "N/A",
                tierName,
                tostring(fishPrice or 0),
                fishMutation or "None",
                os.date("%Y-%m-%d %H:%M:%S")
            ),
            thumbnail = imageUrl and { url = imageUrl } or nil,
            footer = { text = "ArtHub Fishing Bot" },
            timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
        } }
    }

    pcall(function()
        local req = (syn and syn.request) or (http and http.request) or http_request or request
        req({
            Url = WEBHOOK_URL,
            Method = "POST",
            Headers = { ["Content-Type"] = "application/json" },
            Body = SERVICES.HttpService:JSONEncode(payload)
        })
    end)
end


WebhookSection:AddInput({
    Title = "Webhook URL",
    Placeholder = "Paste Discord Webhook Here...",
    Callback = function(v)
        WEBHOOK_URL = v
    end
})

WebhookSection:AddDropdown({
    Title = "Filter Tiers",
    Options = { "Common", "Uncommon", "Rare", "Epic", "Legendary", "Mythic", "SECRET" },
    Default = { "Mythic", "SECRET" },
    Multi = true,
    Callback = function(selected)
        rareti = selected
    end
})

WebhookSection:AddToggle({
    Title = "Enable Webhook Notification",
    Default = false,
    Callback = function(v)
        webactive = v

        -- Memutuskan koneksi lama jika ada
        if hookConnection then
            hookConnection:Disconnect()
            hookConnection = nil
        end

        if v then
            -- Pastikan remote tersedia sebelum connect
            local event = Remote.RE.ObtainedNewFishNotification
            if event then
                hookConnection = event.OnClientEvent:Connect(function(fishId, fishData)
                    if not webactive then return end

                    -- Cari data statis ikan dari folder Items
                    local staticData = nil
                    for _, module in ipairs(loadData.Items:GetChildren()) do
                        if module:IsA("ModuleScript") then
                            local success, data = pcall(require, module)
                            if success and data.Data and data.Data.Id == fishId then
                                staticData = data
                                break
                            end
                        end
                    end

                    if staticData then
                        local tierName = getTierName(staticData.Data.Tier)

                        -- Cek apakah tier masuk dalam filter
                        if table.find(rareti, tierName) then
                            sendFishWebhook(
                                staticData.Data.Name,
                                tierName,
                                fishData.Weight,
                                fishData.VariantId,
                                staticData.SellPrice,
                                staticData.Data.Icon
                            )
                        end
                    end
                end)
            end
        end
    end
})
