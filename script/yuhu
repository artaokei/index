        PlayersTeleList:UpdateOptions(listPlayersTele);
        arthub("Refresh Successfully!");
    end
});
PlayersTele:AddButton({
    Title = "Teleport Now",
    Callback = function()
        if not currentSelectedPlayer or currentSelectedPlayer == "No Players Found" then
            arthub("Error: Please select a player first.");
            return
        end

        local targetPlayer = SERVICES.Players:FindFirstChild(currentSelectedPlayer)

        if not targetPlayer then
            arthub("Error: Target player " .. currentSelectedPlayer .. " not found in game.");
            return
        end

        local targetHRP = targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")

        if not targetHRP and targetPlayer.Character then
            targetHRP = targetPlayer.Character:WaitForChild("HumanoidRootPart", 5)
        end

        if targetHRP then
            TeleportTo(targetHRP.Position + Vector3.new(0, 5, 0))
            arthub("Teleport to " .. currentSelectedPlayer .. " Successfully!");
        else
            arthub("Error: Character or HumanoidRootPart not found for " .. currentSelectedPlayer .. ".");
        end
    end
});

