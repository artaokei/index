-- FUNCTION
local function TeleportTo(CFrameOrVector)
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait();
    local HRP = char:FindFirstChild("HumanoidRootPart");
    if HRP then
        if typeof(CFrameOrVector) == "Vector3" then
            HRP.CFrame = CFrame.new(CFrameOrVector);
        elseif typeof(CFrameOrVector) == "CFrame" then
            HRP.CFrame = CFrame.new(CFrameOrVector.p);
        end
    end
end

local function TeleportToLookAt(position, lookVector)
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait();
    local HRP = char:FindFirstChild("HumanoidRootPart");
    if HRP then
        HRP.CFrame = CFrame.new(position, position + lookVector);
    end
end

local function GetPlayerList()
    local players = SERVICES.Players:GetPlayers();
    local list = {};
    for _, player in ipairs(players) do
        if player.Name ~= LocalPlayer.Name then
            table.insert(list, player.Name);
        end
    end
    return list;
end

local listPlayersTele = GetPlayerList();

local function GetFishCount()
    local a = SERVICES.PG:WaitForChild("Inventory"):WaitForChild("Main"):WaitForChild("Top")
        :WaitForChild("Options"):WaitForChild("Fish"):WaitForChild("Label"):WaitForChild("BagSize");
    return tonumber((a.Text or "0/???"):match("(%d+)/")) or 0;
end

local function EquipRod()
    local function IsRodEquipped()
        local equippedId = loadData.Data:Get("EquippedId")
        if not equippedId then
            return false
        end
        local item = MODULES.PlayerStatsUtility:GetItemFromInventory(loadData.Data, function(data)
            return data.UUID == equippedId
        end)
        if not item then
            return false
        end
        local itemData = MODULES.ItemUtility:GetItemData(item.Id)
        return itemData and itemData.Data.Type == "Fishing Rods"
    end
    if not IsRodEquipped() then
        Remote.RE.EquipTool:FireServer(1)
    end
end

local function InstantFishing()
    _G.Celestial.InstantCount = GetFishCount();
    task.spawn(function()
        while _G.InstantStatus do
            local v195, _, v197 = pcall(function()
                return Remote.RF.ChargeRod:InvokeServer(workspace:GetServerTimeNow());
            end);
            do
                local l_v197_0 = v197;
                if v195 and typeof(l_v197_0) == "number" then
                    local v199 = -1;
                    local v200 = 0.999;
                    task.wait(0.3);
                    do
                        local l_v199_0, l_v200_0 = v199, v200;
                        pcall(function()
                            Remote.RF.StartMini:InvokeServer(l_v199_0, l_v200_0, l_v197_0);
                        end);
                        local v203 = tick();
                        repeat
                            task.wait(0.05);
                        until _G.FishMiniData and _G.FishMiniData.LastShift or tick() - v203 > 1;
                        task.wait(_G.InstantDelay);
                        pcall(function()
                            Remote.RE.FishDone:FireServer();
                        end);
                        local v204 = GetFishCount();
                        local v205 = tick();
                        repeat
                            task.wait(0.05);
                        until v204 < GetFishCount() or tick() - v205 > 1;
                    end;
                end;
            end;
            task.wait(0.05);
        end;
    end);
end

