local function GetTierColor(t)
    return ({
        Common = 0xbdc3c7,
        Uncommon = 0x2ecc71,
        Rare = 0x3498db,
        Epic = 0x9b59b6,
        Legendary = 0xffff00,
        Mythic = 0xff0000,
        SECRET = 0x00ffcc
    })[t] or 0x34495e
end

local function format(n)
    return tostring(n):reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end

local function getThumb(idIcon)
    if not idIcon or idIcon == 0 then return LOGO_URL end
    local cleanId = tostring(idIcon):match("%d+")
    if not cleanId then return LOGO_URL end

    local apiUrl = "https://thumbnails.roblox.com/v1/assets?assetIds=" ..
        cleanId .. "&type=Asset&size=420x420&format=Png"
    local finalUrl = LOGO_URL

    pcall(function()
        local response = game:HttpGet(apiUrl)
        local data = Services.HttpService:JSONDecode(response)
        if data and data.data and data.data[1] and data.data[1].imageUrl then
            finalUrl = data.data[1].imageUrl
        end
    end)
    return finalUrl
end

local function send(payload)
    if WEBHOOK_URL == "" then return end
    ((syn and syn.request) or request)({
        Url = WEBHOOK_URL,
        Method = "POST",
        Headers = {["Content-Type"]="application/json"},
        Body = Services.HttpService:JSONEncode(payload)
    })
end

