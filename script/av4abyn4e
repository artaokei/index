                })
                task.wait(inventoryDelayHours * 3600)
            end
        end)
    end
})

local function GetMutation(catch, static)
    local m = {}
    if catch.VariantId and catch.VariantId ~= "" then table.insert(m, tostring(catch.VariantId)) end
    if static.Weight and catch.Weight and catch.Weight >= static.Weight.Big.Min then table.insert(m,"Big") end
    if catch.Shiny then table.insert(m,"Shiny") end
    return #m > 0 and table.concat(m," + ") or "None"
end

Remote.NewFishNotif.OnClientEvent:Connect(function(id, catch)
    if not isLoggingActive then return end

    for _, m in ipairs(Services.RS.Items:GetChildren()) do
        local ok, data = pcall(require, m)
        if ok and data.Data and data.Data.Id == id then
            
            -- Ambil data & proteksi jika nil
            local rawTier = data.Data.Tier or "Unknown"
            local tier = tierMapping[rawTier] or rawTier
            local fishName = data.Data.Name or "Unknown Fish"
            
            -- Filter Tier & Name
            if next(selectedTiers) and not selectedTiers[tier] then return end
            if next(selectedNames) and not selectedNames[fishName] then return end

            local mut = GetMutation(catch, data) or "None" -- Proteksi nil
            
            -- Filter Mutation
            if next(selectedMutations) then
                local pass = false
                for k in pairs(selectedMutations) do
                    if mut:lower():find(k:lower()) then pass = true end
                end
                if not pass then return end
            end

            -- Pastikan fungsi format tersedia, jika tidak pakai tostring
            local function safeFormat(val)
                return (type(format) == "function") and format(val) or tostring(val)
            end

            -- Kirim Webhook
            send({
                username = "ArtHub Notify",
                avatar_url = LOGO_URL,
                embeds = {{
                    title = " Fish Caught!",
                    color = (type(GetTierColor) == "function") and GetTierColor(tier) or 0xFFFFFF,
                    thumbnail = { url = getThumb(data.Data.Icon) or "" },
                    fields = {
                        {name = " Player", value = "```" .. (Services.LocalPlayer.DisplayName or "Unknown") .. "```", inline = true},
                        {name = " Fish", value = "```" .. fishName .. "```", inline = true},
                        {name = " Tier", value = "```" .. tier .. "```", inline = true},
                        {name = " Mutation", value = "```" .. mut .. "```", inline = true},
                        {name = " Weight", value = "```" .. safeFormat(catch.Weight or 0) .. " Kg```", inline = true},
                        {name = " Price", value = "```$" .. safeFormat(data.SellPrice or 0) .. "```", inline = true}
                    },
                    footer = { text = "ArtHub Logging System" },
                    timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
                }}
            })
        end
    end
end)

