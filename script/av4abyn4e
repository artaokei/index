            noClipConnection = nil
        end

        DisableV3Physics()
    end
end)

local waterPlatform = nil
local walkOnWaterConnection = nil

local listEvent = {
    "Megalodon Hunt",
    "Ghost Shark Hunt",
    "Shark Hunt",
    "Worm Hunt",
    "Treasure Hunt"
}
local SelectEventTele = "No Event"

TeleEvent:AddDropdown({
    Title = "Select Event",
    Options = listEvent,
    Default = SelectEventTele,
    Callback = function(name)
        SelectEventTele = name
    end
})

TeleEvent:AddToggle({
    Title = "Start Auto Event",
    Default = false,
    Callback = function(val)
        _G.AutoEventLoop = val
        
        if _G.AutoEventLoop then
            local hasTeleported = false
            local oldPosition = nil -- Variabel untuk menyimpan posisi awal

            task.spawn(function()
                while _G.AutoEventLoop do
                    local character = Services.LocalPlayer.Character
                    local hrp = character and character:FindFirstChild("HumanoidRootPart")
                    
                    if hrp then
                        -- 1. Logika Walk On Water
                        if not waterPlatform then
                            waterPlatform = Instance.new("Part")
                            waterPlatform.Name = "WaterPlatform"
                            waterPlatform.Anchored = true
                            waterPlatform.CanCollide = true
                            waterPlatform.Transparency = 1
                            waterPlatform.Size = Vector3.new(25, 1, 25)
                            waterPlatform.Parent = workspace
                        end

                        if not walkOnWaterConnection then
                            walkOnWaterConnection = Services.RunService.RenderStepped:Connect(function()
                                if not _G.AutoEventLoop or not hrp or not waterPlatform then return end
                                local rayParams = RaycastParams.new()
                                rayParams.FilterDescendantsInstances = { workspace.Terrain }
                                rayParams.FilterType = Enum.RaycastFilterType.Include
                                rayParams.IgnoreWater = false

                                local result = workspace:Raycast(hrp.Position + Vector3.new(0, 5, 0), Vector3.new(0, -500, 0), rayParams)
                                if result and result.Material == Enum.Material.Water then
                                    local waterY = result.Position.Y
                                    waterPlatform.Position = Vector3.new(hrp.Position.X, waterY, hrp.Position.Z)
                                    if hrp.Position.Y < waterY + 2 and hrp.Position.Y > waterY - 5 then
                                        if not UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                                            hrp.CFrame = CFrame.new(hrp.Position.X, waterY + 3.2, hrp.Position.Z)
                                        end
                                    end
                                else
                                    waterPlatform.Position = Vector3.new(hrp.Position.X, -500, hrp.Position.Z)
                                end
                            end)
                        end

                        -- 2. Logika Auto Teleport & Auto Return
                        if SelectEventTele ~= "No Event" then
                            local targetObj = workspace:FindFirstChild(SelectEventTele, true) or 
                                             (workspace:FindFirstChild("Props") and workspace.Props:FindFirstChild(SelectEventTele, true))
                            
                            if targetObj and not hasTeleported then
                                -- Simpan posisi sebelum pergi
                                oldPosition = hrp.CFrame
                                
                                -- Teleport ke Event (+50 Studs)
                                hrp.CFrame = targetObj:GetPivot() * CFrame.new(0, 50, 0)
                                hasTeleported = true
                                
                                task.delay(0.5, function()
                                    local prompt = targetObj:FindFirstChildWhichIsA("ProximityPrompt", true)
                                    if prompt then fireproximityprompt(prompt) end
                                end)
                                
                            elseif not targetObj and hasTeleported then
                                -- JIKA EVENT HABIS (Objek hilang)
                                if oldPosition then
                                    hrp.CFrame = oldPosition -- Balik ke posisi semula
                                    oldPosition = nil -- Reset posisi simpanan
                                end
                                hasTeleported = false
                            end
                        end
                    end
                    task.wait(1)
                end
            end)
        else
            -- 3. Cleanup
            if walkOnWaterConnection then
                walkOnWaterConnection:Disconnect()
                walkOnWaterConnection = nil
            end
            if waterPlatform then
                waterPlatform:Destroy()
                waterPlatform = nil
            end
        end
    end
})


task.spawn(RefreshMerchant)
task.spawn(function()
    while true do
        if Config.Auto.Favorite.Enabled then
            pcall(ProcessAutoFavorite)
        end
        task.wait(3)
    end
end)
